SetOfAuthenticatedAttribute ::= SET OF AuthenticatedAttribute

AuthenticatedAttribute ::= SEQUENCE {
	type			OBJECT IDENTIFIER ({ lc_pkcs7_external_aa_OID }),
	values			SET OF ANY ({ lc_pkcs7_external_aa })
} ({ lc_pkcs7_external_aa_continue })
